game multiplicacion_matriz;
canvas = 10, 10;

declare {
    int[10, 10] mat_a, mat_b, mat_res;
}

func Start : void () {
    declare {
        int m, n, p;
        int i, j, k;
    }

    Print("MultiplicaciÃ³n de matrices -> M(m x n) * M(n x p) = M(m x p)\n");

    m = 0;
    while (m <= 0 o m > 10) {
        Print("Dimension m (0 < m <= 10): ");
        Read(m);
    }

    n = 0;
    while (n <= 0 o n > 10) {
        Print("Dimension n (0 < n <= 10): ");
        Read(n);
    }

    p = 0;
    while (p <= 0 o p > 10) {
        Print("Dimension p (0 < p <= 10): ");
        Read(p);
    }

    Print("Rellena matriz A:\n");
    for (i = 0; i < m; i = i + 1) {
        for (j = 0; j < n; j = j + 1) {
            Print("MatA [");
            Print(i);
            Print(", ");
            Print(j);
            Print("]: ");
            Read(mat_a[i, j]);
        }
    }

    Print("Rellena matriz B:\n");
    for (i = 0; i < n; i = i + 1) {
        for (j = 0; j < p; j = j + 1) {
            Print("MatB[");
            Print(i);
            Print(", ");
            Print(j);
            Print("]: ");
            Read(mat_b[i, j]);
        }
    }

    Print("Multiplicando matrices ando\n");
    for (i = 0; i < m; i = i + 1) {
        for (j = 0; j < p; j = j + 1) {
            for (k = 0; k < n; k = k + 1) {
                mat_res[i, j] = mat_res[i, j] + mat_a[i, k] * mat_b[k, j];
            }
        }
    }


    Print("Resultado de MatA x MatB:\n");
    for (i = 0; i < m; i = i + 1) {
        for (j = 0; j < p; j = j + 1) {
            Print("MatC[");
            Print(i);
            Print(", ");
            Print(j);
            Print("]: ");
            Print(mat_res[i, j]);
            Print("\n");
        }
    }
}

func Update : void () {
    game_over;
}